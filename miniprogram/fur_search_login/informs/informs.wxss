.line{
  border-bottom:2rpx dashed #a9a9a9;
  height:5vh;
  width:100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 20rpx;
}

.content{
  width:80%;
  height:100%;
  display: flex;
  align-items: center;
  margin-left:20rpx;

}
.font{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:25rpx;
  font-weight: 600;
}